{% extends "base.html.twig" %}
{% block title %}
    Bienvenu sur votre profil : {{ app.user.username }}
{% endblock %}
{% block body %}
<div class="row">
    <div class="col-md-4 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Table Candidat</h4>
                    <ul>
                    <li>{{ app.user.id }}</li>
                    <li>{{ app.user.email }}</li>
                    <li>{{ app.user.username }}</li>
                    <li> <a href="{{ path('app_profilUpdate',{'id': app.user.id} ) }}" class="btn btn-primary"> Modifier profil </a></li>
                    </ul>
            </div>
        </div>
    </div> 
   
   
      
</div>                 
{% endblock %}
